// Skeleton Settings SPEED=75;                // 35-1000 ... lower values are fasterPOISONOUS=0;             // 0-100 ... higher values are more poisonousHIT_POINTS=45;           // 1-250  75STRENGTH=75;             // 1-250INTELLIGENCE=60;         // 1-250DEXTERITY=50;            // 1-250MAGIC_POINTS=NA;         // 1-250MIN_GOLD=10;             // 0 - 100000MAX_GOLD=30;             // 0 - 100000SEEK_GOLD=TRUE;          // TRUE or FALSE ... creature will pick up goldSEEK_MAGIC=FALSE;        // TRUE or FALSE ... creature will pick up magic itemsSEEK_WEAPONS=TRUE;       // TRUE or FALSE ... creature will pick up weaponsMAGIC_RESIST=90;         // 0 - 100 ... lower is more resistent 70TAMING_LEVEL=101;        // 1 - 101 ... lower is easier to tame, 101 is impossibleARMOR_RATING=40;         // 1 - 100  30POISON_IMMUNITY=TRUE;    // TRUE or FALSELOCK=2;                  // 0 - 100MIN_DAMAGE=4;            // 0 - 250MAX_DAMAGE=8;           // 0 - 250LEVEL=4;TRACKING_LEVEL=4;SIZE=1;// Weapon Effectivness#dagger(75);#holydagger(125);#blooddagger(75);#broadsword(75);#spear(75);#shortbow(20);#longbow(20);#longsword(75);#glaive(75);#mace(100);#warhammer(110);#flail(110);// Creature Inventory// Inventory command:////  For grouped items, like potions://  #inventory(ITEM_NUMBER,MINIMUM,MAXIMUM);////  For ungrouped items, like weapons://  #inventory(ITEM_NUMBER,1,ODDS); // 1 in ODDS chance of carrying item#inventory(378,1,1); // 1 skull#inventory(166,1,200); // holy dagger