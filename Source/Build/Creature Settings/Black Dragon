// Black Dragon SettingsSPEED=50;                // 35-1000 ... lower values are fasterPOISONOUS=0;             // 0-100 ... higher values are more poisonousHIT_POINTS=250;          // 1-250STRENGTH=250;            // 1-250INTELLIGENCE=250;        // 1-250DEXTERITY=250;           // 1-250MAGIC_POINTS=NA;         // 1-250MIN_GOLD=200;            // 0 - 100000MAX_GOLD=500;            // 0 - 100000SEEK_GOLD=TRUE;          // TRUE or FALSE ... creature will pick up goldSEEK_MAGIC=FALSE;        // TRUE or FALSE ... creature will pick up magic itemsSEEK_WEAPONS=TRUE;       // TRUE or FALSE ... creature will pick up weaponsMAGIC_RESIST=0;          // 0 - 100 ... lower is more resistentTAMING_LEVEL=101;        // 1 - 101 ... lower is easier to tame, 101 is impossibleARMOR_RATING=100;        // 1 - 100POISON_IMMUNITY=TRUE;    // TRUE or FALSELOCK=5;                  // 0 - 100MIN_DAMAGE=250;          // 0 - 250MAX_DAMAGE=250;          // 0 - 250LEVEL=15;TRACKING_LEVEL=15;SIZE=3;// Weapon Effectivness#dagger(10);#holydagger(10);#blooddagger(10);#broadsword(10);#katana(10);#scimitar(10);#flail(10);#mace(10);#warhammer(10);#spear(10);#sickle(10);#shortbow(10);// Creature Inventory// Inventory command:////  For grouped items, like potions://  #inventory(ITEM_NUMBER,MINIMUM,MAXIMUM);////  For ungrouped items, like weapons://  #inventory(ITEM_NUMBER,1,ODDS); // 1 in ODDS chance of carrying item#inventory(352,20,30); // 20 to 30 raw steaks#inventory(275,10,20); // 10 to 20 dragon teeth#inventory(379,15,25); // 15 to 25 dragon scales