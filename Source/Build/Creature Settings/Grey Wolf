// Grey Wolf SettingsSPEED=80;                // 35-1000 ... lower values are fasterPOISONOUS=0;             // 0-100 ... higher values are more poisonousHIT_POINTS=15;           // 1-250STRENGTH=15;             // 1-250INTELLIGENCE=25;         // 1-250DEXTERITY=75;            // 1-250MAGIC_POINTS=NA;         // 1-250MIN_GOLD=0;              // 0 - 100000MAX_GOLD=0;              // 0 - 100000SEEK_GOLD=FALSE;         // TRUE or FALSE ... creature will pick up goldSEEK_MAGIC=FALSE;        // TRUE or FALSE ... creature will pick up magic itemsSEEK_WEAPONS=FALSE;      // TRUE or FALSE ... creature will pick up weaponsMAGIC_RESIST=100;        // 0 - 100 ... lower is more resistentTAMING_LEVEL=3;          // 1 - 101 ... lower is easier to tame, 101 is impossibleARMOR_RATING=10;         // 1 - 100POISON_IMMUNITY=FALSE;   // TRUE or FALSELOCK=0;                  // 0 - 100MIN_DAMAGE=6;            // 0 - 250MAX_DAMAGE=12;            // 0 - 250LEVEL=3;TRACKING_LEVEL=3;SIZE=1;// Weapon Effectiveness#katana(90);#scimitar(90);#longsword(90);#mace(90);#warhammer(90);#sickle(90);#stave(90);#longbow(90);#longsword(75);#battleaxe(75);#glaive(75);// Creature Inventory// Inventory command:////  For grouped items, like potions://  #inventory(ITEM_NUMBER,MINIMUM,MAXIMUM);////  For ungrouped items, like weapons://  #inventory(ITEM_NUMBER,1,ODDS); // 1 in ODDS chance of carrying item#inventory(352,1,3); // 1 to 3 raw steaks#inventory(311,2,3); // 2 to 3 furs